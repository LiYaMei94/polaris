/* pages/mine_vip_rule/mine_vip_rule.wxss */

.header {
  display: flex;
  background-color: white;
  height: 92rpx;
}

.header-item {
  flex: 1;
  text-align: center;
  font-size: 30rpx;
}

.header-enable {
  color: #324d56;
  line-height: 88rpx;
  display: inline-block;
  border-bottom: solid 4rpx #324d56;
}

.header-disable {
  color: #333;
  line-height: 88rpx;
}

/* 滑动内容区域 */
.content{
  height: 100rpx;
  flex: 1;
  display: flex;
  flex-direction: column;
}
.vip {
  width: 100%;
}

.jifen {
  width: 100%;
}
