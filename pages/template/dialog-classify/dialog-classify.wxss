.dialog-classify{
  position: absolute;
  display: flex;
  z-index: 10;
  top: 70rpx;
  right: 30rpx;
}

.classify-back{
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: 9;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
}

.classify-content{
  display: flex;
  flex-direction: column;
  width: 300rpx;
  height: 161rpx;

  z-index: 20;
}

.classify-content-back{
  width: 300rpx;
  height: 161rpx;
  position: absolute;
}

.classify-content-center{
  width: 300rpx;
  height: 161rpx;

  display: flex;
  flex-direction: column;

  z-index: 30;
  margin-top: 7rpx;
}

.classify-content-center text{
  display: block;
  height: 76rpx;
  width: 100%;
  text-align: center;
  line-height: 76rpx;

  font-size: 26rpx;
  color: #999999;
}

.classify-content-center-line{
  width: 100%;
  height: 2rpx;
  background-color: #EEEEEE;
}